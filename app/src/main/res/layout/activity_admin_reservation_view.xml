<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:orientation="vertical"
    tools:context=".Reservation.adminReservationView">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/appointments"
            android:textSize="30sp"
            android:layout_gravity="center"
            android:textColor="@color/primary"
            android:fontFamily="@font/adamina"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/dateContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="20dp">

        <ImageView
            android:id="@+id/previousBtn"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center_vertical"
            android:background="@drawable/baseline_arrow_back_ios_24"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:contentDescription="@string/previous" />

        <TextView
            android:id="@+id/dateDisplay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/month_day_year"
            android:fontFamily="@font/adamina"
            android:textColor="@color/text"
            android:textSize="25sp"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp" />

        <ImageView
            android:id="@+id/nextBtn"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center_vertical"
            android:background="@drawable/forward1"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="20dp"
            android:contentDescription="@string/next" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/timeDisplay1"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/border"
        android:padding="10dp"
        android:orientation="horizontal"
        android:gravity="center_horizontal"
        android:layout_marginBottom="20dp">

        <TextView
            android:id="@+id/time1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/_9_00_am"
            android:fontFamily="@font/adamina"
            android:textColor="@color/text"
            android:textSize="25sp"/>

        <TextView
            android:id="@+id/limit1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/_0_20"
            android:fontFamily="@font/adamina"
            android:textColor="@color/text"
            android:textSize="25sp"
            android:layout_marginStart="10dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/timeDisplay2"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/border"
        android:padding="10dp"
        android:orientation="horizontal"
        android:gravity="center_horizontal"
        android:layout_marginBottom="20dp">

        <TextView
            android:id="@+id/time2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/_10_00_am"
            android:fontFamily="@font/adamina"
            android:textColor="@color/text"
            android:textSize="25sp"/>

        <TextView
            android:id="@+id/limit2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/_0_20"
            android:fontFamily="@font/adamina"
            android:textColor="@color/text"
            android:textSize="25sp"
            android:layout_marginStart="10dp"/>
    </LinearLayout>

    <LinearLayout
